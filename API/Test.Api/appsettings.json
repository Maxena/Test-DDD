{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ApiSettings": {
        "JwtSettings": {
            "Secret": "secret",
            "Expiration": "1h"
        },
        "SwaggerSettings": {
            "Title": "API",
            "Description": "API",
            "Version": "1.0.0",
            "TermsOfService": "None",
            "Contact": {
                "Name": "API",
                "Url": "",
                "Email": ""
            },
            "License": {
                "Name": "MIT",
                "Url": "https://opensource.org/licenses/MIT"
            }
        },
        "CorsSettings": {
            "AllowedOrigins": "*",
            "AllowedHeaders": "*",
            "AllowedMethods": "*",
            "ExposedHeaders": "*",
            "AllowCredentials": true
        },
        "ConnectionStrings": {
            "DefaultConnection": "Server=localhost;Database=api;Trusted_Connection=True;MultipleActiveResultSets=true"
        },
        "IdentitySettings": {
            "Password": {
                "RequireDigit": true,
                "RequireLowercase": true,
                "RequireNonAlphanumeric": true,
                "RequireUppercase": true,
                "RequiredLength": 6,
                "RequiredUniqueChars": 1
            }
        }
    }
}